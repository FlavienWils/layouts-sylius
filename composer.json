{
    "name": "netgen/block-manager-sylius",
    "description": "Netgen Block Manager & Sylius eCommerce integration",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://www.netgenlabs.com"
        }
    ],
    "require": {
        "netgen/block-manager": "~0.8.0@dev",
        "sylius/sylius": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/phpunit-bridge": "^2.8|^3.2",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "franzl/studio": "*"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always"
    },
    "repositories": [
        { "type": "composer", "url": "https://packagist.netgen.biz" }
    ],
    "autoload": {
        "psr-4": {
            "Netgen\\BlockManager\\Sylius\\": "lib/",
            "Netgen\\Bundle\\SyliusBlockManagerBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ],
        "psr-4": {
            "Netgen\\BlockManager\\Sylius\\Tests\\": "tests/lib/",
            "Netgen\\Bundle\\SyliusBlockManagerBundle\\Tests\\": "tests/bundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.8.x-dev"
        }
    }
}
